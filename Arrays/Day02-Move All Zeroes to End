

// User function Template for Java

class Solution {
    void pushZerosToEnd(int[] arr) {
        // code here
        int j=-1;
        for(int i=0; i<arr.length;i++){
            if(arr[i]!=0){
                j++;
                arr[j]=arr[i];
            }
        }
        for(int i=j+1;i<arr.length;i++){
            arr[i]=0;
        }
    }
}
