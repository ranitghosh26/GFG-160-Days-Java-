class Solution {
    public long subarrayXor(int arr[], int k) {
        // code here
        long res =0 ;
        long prefXOR = 0;
        Map<Long, Integer> map = new HashMap<>();
        map.put(0L, 1);
        for(int val : arr){
            prefXOR ^= val;
            res += map.getOrDefault(prefXOR ^ k,0);
            map.put(prefXOR,map.getOrDefault(prefXOR, 0) + 1);
        }
        
        return res;
    }
}
